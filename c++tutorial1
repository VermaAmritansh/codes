  #include <iostream>
using namespace std;
int main()
{
	
	char myCharacter;
	myCharacter = 'y';
	int myInt;
	myInt = 13;
	cout << myCharacter << "\n";
	cout << endl;
	cout << myInt << endl;
	myCharacter = 'n';
	myInt = 12;
	cout << myCharacter << '\n';
	cout << myInt << endl;
int a(1);
	int b = 5;
	if (b < a)
		cout << "b less than a";
	else
		cout << "b is not less than a"<< endl;
	system("pause");
	
}
